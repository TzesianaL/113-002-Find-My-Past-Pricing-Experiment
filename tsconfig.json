{
  "compilerOptions": {
    "allowJs": false,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "isolatedModules": true,
    "jsx": "react-jsx",
    "lib": ["dom", "dom.iterable", "esnext"],
    "module": "ESNext",
    "moduleResolution": "node",
    "noEmit": true,
    "noFallthroughCasesInSwitch": true,
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "strict": false,
    "noImplicitAny": false,
    "target": "ESNext",
    "baseUrl": ".",
    "paths": {
      "Components/*": ["src/Components/*"],
      "hooks/*": ["src/hooks/*"],
      "assets/*": ["src/assets/*"],
      "functions/*": ["src/functions/*"],
      "images/*": ["src/images/*"],
      "routing/*": ["src/routing/*"],
      "shared/*": ["src/shared/*"],
      "types": ["src/types/index"],
      "types/*": ["src/types/*"],
      "AppComponents/*": ["src/AppComponents/*"],
      "output/*": ["src/output/*"],
      "StandardLayouts/*": ["src/StandardLayouts/*"],
      "stories/*": ["src/stories/*"],
      "redux/store": ["src/redux/store"],
      "redux/reducers": ["src/redux/reducers"],
      "redux/reducers/*": ["src/redux/reducers/*"],
      "redux/globalActions/globalActions": [
        "src/redux/globalActions/globalActions"
      ]
    }
  },
  "include": ["src"]
}
